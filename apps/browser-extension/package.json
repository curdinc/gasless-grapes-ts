{
  "name": "@gg/browser-extension",
  "displayName": "Gasless Grapes Crypto Wallet",
  "version": "0.0.1",
  "description": "Gasless Grapes is the power user's crypto wallet of choice for all of web3",
  "homepage": "https://gaslessgrapes.com",
  "author": "Gasless Grapes",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.2",
    "@chakra-ui/theme-tools": "^2.0.16",
    "@elasticbottle/trpc-post-message": "^0.0.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@gg/utils": "*",
    "@plasmohq/storage": "^1.3.1",
    "@trpc/client": "^10.14.0",
    "@trpc/server": "^10.14.0",
    "framer-motion": "^10.6.0",
    "plasmo": "0.66.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0",
    "superjson": "1.9.1",
    "trpc-chrome": "^1.0.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint-config-gaslessgrapes": "*",
    "prettier": "2.8.3",
    "typed-emitter": "^2.1.0",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "commands": {
      "accept": {
        "suggested_key": {
          "default": "Alt+Shift+1"
        },
        "description": "Accept the current selection."
      },
      "reject": {
        "suggested_key": {
          "default": "Alt+Shift+W"
        },
        "description": "Reject the current selection."
      },
      "review": {
        "suggested_key": {
          "default": "Alt+Shift+V"
        },
        "description": "Review the current selection."
      },
      "_execute_action": {
        "suggested_key": {
          "windows": "Alt+Shift+Q",
          "mac": "Alt+Shift+Q",
          "chromeos": "Alt+Shift+Q",
          "linux": "Alt+Shift+Q"
        }
      }
    }
  }
}
